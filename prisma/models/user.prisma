model User {
  id           String      @id @default(uuid())
  email        String      @unique
  password     String
  refreshToken String?
  englishLvl   EnglishLvl?
  deck         Deck[]
  interests    Interest[]
  learnedWords String[]
  createdAt    DateTime    @default(now())
  updatedAt    DateTime    @updatedAt
}
