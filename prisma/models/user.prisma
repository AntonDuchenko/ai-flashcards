model User {
  id            String      @id @default(uuid())
  email         String      @unique
  password      String
  refreshToken  String?
  dailyComplete Boolean     @default(false)
  daysStreak    Int         @default(0)
  englishLvl    EnglishLvl?
  decks         Deck[]
  interests     Interest[]
  learnedWords  Flashcard[]
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @updatedAt
}
