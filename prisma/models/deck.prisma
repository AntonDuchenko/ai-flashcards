model Deck {
  id         String     @id @default(uuid())
  title      String
  userId     String
  englishLvl EnglishLvl
  user       User       @relation(fields: [userId], references: [id])
  flashcards Json
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
}

enum EnglishLvl {
  A1
  A2
  B1
  B2
  C1
  C2
}
